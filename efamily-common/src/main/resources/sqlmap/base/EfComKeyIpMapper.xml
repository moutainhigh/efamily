<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">


<mapper namespace="com.winterframework.efamily.entity.EfKeyIp">
	<resultMap id="EfKeyIpResult" type="com.winterframework.efamily.entity.EfKeyIp">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="efKeyIpColumns">
	    <![CDATA[
	        	id as id,
	        	ukey as ukey,
	        	ip as ip,
	        	remark as remark,
	        	creator_id as creatorId,
	        	create_time as createTime,
	        	updator_id as updatorId,
	        	update_time as updateTime
	    ]]>
	</sql>
		<!-- useGeneratedKeys="true" keyProperty="xxx" for sqlserver and mysql -->
		<insert id="insert" parameterType="com.winterframework.efamily.entity.EfKeyIp" 
			useGeneratedKeys="true" keyProperty="id" 
		 flushCache="true">
	    <![CDATA[
	        INSERT INTO
	        ef_key_ip (
	        	id ,
	        	ukey ,
	        	ip ,
	        	remark ,
	        	creator_id ,
	        	create_time  
	        ) VALUES (
	        	#{id,javaType=long,jdbcType=INTEGER} ,
	        	#{ukey,javaType=string,jdbcType=VARCHAR} ,
	        	#{ip,javaType=string,jdbcType=VARCHAR} ,
	        	#{remark,javaType=string,jdbcType=VARCHAR} ,
	        	#{creatorId,javaType=long,jdbcType=DECIMAL} ,
	        	#{createTime,javaType=date,jdbcType=TIMESTAMP}  
	        )
	    ]]>	
	
		</insert>
	    
		<update id="update" parameterType="com.winterframework.efamily.entity.EfKeyIp">
	        UPDATE ef_key_ip SET
			        <if test="@Ognl@isNotEmpty(ukey)">
					ukey = #{ukey,javaType=string,jdbcType=VARCHAR} ,
				    </if>
			        <if test="@Ognl@isNotEmpty(ip)">
					ip = #{ip,javaType=string,jdbcType=VARCHAR} ,
				    </if>
			        <if test="@Ognl@isNotEmpty(remark)">
					remark = #{remark,javaType=string,jdbcType=VARCHAR} ,
				    </if>
			        <if test="@Ognl@isNotEmpty(updatorId)">
					updator_id = #{updatorId,javaType=long,jdbcType=DECIMAL} ,
				    </if>
			        <if test="@Ognl@isNotEmpty(updateTime)">
					update_time = #{updateTime,javaType=date,jdbcType=TIMESTAMP} 
				    </if>
	        WHERE 
		        id = #{id} 
		</update>
	   <delete id="delete" parameterType="java.lang.Long">
	    <![CDATA[
	        delete from ef_key_ip where
		        id = #{id} 
	    ]]>
	    </delete>   
	    <select id="getById" parameterType="long" resultMap="EfKeyIpResult" flushCache="false">
			select <include refid="efKeyIpColumns" />
		    <![CDATA[
			    from ef_key_ip 
		        where 
		        id = #{id} 
		    ]]>
		</select>	
		<select id="getByIds" parameterType="list"  resultMap="EfKeyIpResult" flushCache="false">
			select <include refid="efKeyIpColumns" />
			    from ef_key_ip 
		        where id in
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
#{item}				</foreach>					
		</select>	

	<sql id="efKeyIpDynamicWhere">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       <if test="@Ognl@isNotEmpty(id)">
				and id = #{id}
			</if>
	       <if test="@Ognl@isNotEmpty(ukey)">
				and ukey = #{ukey}
			</if>
	       <if test="@Ognl@isNotEmpty(ip)">
				and ip = #{ip}
			</if>
	       <if test="@Ognl@isNotEmpty(remark)">
				and remark = #{remark}
			</if>
	       <if test="@Ognl@isNotEmpty(creatorId)">
				and creator_id = #{creatorId}
			</if>
	       <if test="@Ognl@isNotEmpty(createTime)">
				and create_time = #{createTime}
			</if>
	       <if test="@Ognl@isNotEmpty(updatorId)">
				and updator_id = #{updatorId}
			</if>
	       <if test="@Ognl@isNotEmpty(updateTime)">
				and update_time = #{updateTime}
			</if>
		</where>
	</sql>
	
	<select id="getAll" resultMap="EfKeyIpResult"  flushCache="false">
    	select <include refid="efKeyIpColumns" />
	    from ef_key_ip 

		<if test="@Ognl@isNotEmpty(sortColumns)">
			ORDER BY ${sortColumns}
		</if>
    </select>	
	<select id="getCount" resultType="long">
        select count(*) from ef_key_ip 
    </select>
    <select id="getCountByPage" resultType="long">
        select count(*) from ef_key_ip 
		<include refid="efKeyIpDynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 offset,pageSize,lastRows 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="getByPage" resultMap="EfKeyIpResult">
    	select <include refid="efKeyIpColumns" />
	    from ef_key_ip 
		<include refid="efKeyIpDynamicWhere"/>
		<if test="@Ognl@isNotEmpty(sortColumns)">
			ORDER BY ${sortColumns}
		</if>
    </select>
	<select id="getObjByAttribute" resultMap="EfKeyIpResult">
    	select <include refid="efKeyIpColumns" />
	    from ef_key_ip 
		<include refid="efKeyIpDynamicWhere"/>
    </select>


	
</mapper>